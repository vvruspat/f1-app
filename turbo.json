{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"format": {
			"dependsOn": ["^format"]
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"test:e2e": {
			"dependsOn": ["^test:e2e"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"csstypes": {
			"dependsOn": ["^csstypes"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	},
	"remoteCache": {
		"enabled": true
	}
}
